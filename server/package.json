{
  "name": "senditserver",
  "version": "1.0.0",
  "description": "Server for SendIT app",
  "main": "index.js",
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "test": "./node_modules/.bin/mocha build/v1/tests",
    "lint": "eslint  src --ignore-path .eslintignore --config .eslintrc",
    "build-js": "./node_modules/.bin/babel src -d build",
    "watch-js": "./node_modules/.bin/babel --watch src -d build",
    "cover": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha  build/v1/tests/",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "postinstall": "npm run build-js && node build/v1/helpers/db/setupdb.js"
  },
  "repository": {
    "type": "git",
    "url": "github.com/oneeyedsunday/sendit"
  },
  "author": "Idiakose O. Sunday",
  "license": "ISC",
  "dependencies": {
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.18.3",
    "debug": "^4.1.0",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.3.0",
    "method-override": "^3.0.0",
    "pg": "^7.6.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.6"
  },
  "engines": {
    "node": "10.11.0",
    "npm": "6.4.1"
  }
}
